version: '3'

services:
  hugo:
    container_name: hugo
    image: klakegg/hugo:0.62.1
    command: server
    ports:
      - "1313:1313"
    volumes:
      - ./hugo/hugo_blog/:/src
      - - ./wp-app:/var/www/html
    networks:
      - web
    labels:
      - traefik.frontend.rule=Host:theomarty.fr
